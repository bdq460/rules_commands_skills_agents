        },
        "qwen": {
            "name": "通义千问",
            "baseURL": "https://dashscope.aliyuncs.com/compatible-mode/v1",
            "model": "qwen3-max",
            "apiKey": "sk-c9aac0a02f934409a8bec5d415217a6c"
        },
        "deepseek": {
            "name": "DeepSeek",
            "baseURL": "https://api.deepseek.com/v1",
